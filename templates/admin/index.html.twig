{% extends 'base.html.twig' %}

{% block title %}Hello AdminController!{% endblock %}

{% block body %}
<table>
    <tr>
        <th>Id</th>
        <th>Prénom</th>
        <th>Nom</th>
        <th>Téléphone</th>

    </tr>
    {% for participant in listeParticipants %}
        <tr>
            <td>{{participant.id }}</td>
            <td>{{participant.prenom }}</td>
            <td>{{participant.nom }}</td>
            <td>{{participant.telephone }}</td>

        </tr>
    {%  endfor %}
</table>
{% endblock %}
